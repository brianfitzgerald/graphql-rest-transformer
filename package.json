{
  "name": "graphql-rest-transformer",
  "version": "1.0.2",
  "description":
    "Allows you to query a GraphQL interface like you'd query a REST URL. I wrote this to help me learn GraphQL, but it might have some practical use as well, as a means of ensuring backwards compatibility with REST APIs.",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "dev":
      "concurrently -n \"tsc,server\" \"tsc -w\" \"nodemon build/index.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/express-graphql": "^0.6.1",
    "@types/node": "^9.6.2",
    "@types/graphql": "^0.13.0",
    "axios": "^0.18.0",
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "nodemon": "^1.17.3",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2"
  },
  "keywords": ["graphql", "rest", "json", "express", "node"],
  "author": "Brian Fitzgerald"
}
